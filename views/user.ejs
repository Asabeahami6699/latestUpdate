                    <select id="bank-type" name="BankType" required>
                        <option value="choose" disabled selected hidden>Select An Option</option>
                        <option value="ecobank">Ecobank</option>
                        <option value="fidelity">Fidelity Bank</option>
                        <option value="Calbank">Calbank</option>
                        <option value="Other Banks">Other Banks</option>
                    </select>
                    <label for="description-type">Description Type:</label>
                    <select id="description-type" name="description" required>
                        <option value="choose" disabled selected hidden>Select An Option</option>
                        <option value="Deposit">Deposit</option>
                        <option value="Withdrawal">Withdrawal</option>
                    </select>
                    <label for="account-numb">Slip Number:</label>
                    <input type="number" name="AccountNumber" placeholder="Enter Slip Number Here" required>
                    <label for="name">Name of Beneficial:</label>
                    <input type="text" name="Bname" placeholder="Name" required>
